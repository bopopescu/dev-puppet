# ipv4 and 6 nginx config file

server {
	listen 91.198.174.7:80;
	listen [2620:0:862:1:6666:6666:6666:6666]:80;
	server_name  ipv6and4.labs.wikimedia.org results.labs.wikimedia.org;

	log_format lighttpd '$remote_addr $host $remote_user $time_local "$request" $status $body_bytes_sent "$http_referer" "$http_user_agent"';
	access_log	/var/log/nginx/ipv6and4.access.log lighttpd;
	log_not_found	off;

	location / {
		root	/var/www;
		index  index.html;
		autoindex on;
	}

}

