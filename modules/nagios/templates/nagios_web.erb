define host{
        host_name                       <%= webhost %>
        alias                           web-server
        address                         <%= webip %>
        check_command                   check-host-alive
        max_check_attempts              2
        check_period                    24x7
        contact_groups                  web-server
        notification_interval           2
        notification_period             24x7
        notification_options            d,u,r
        } 
